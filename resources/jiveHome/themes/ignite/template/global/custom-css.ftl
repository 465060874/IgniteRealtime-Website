body {

	text-align: left;

	}

a {

	color: #f06500;

	}

a:hover {

	color: #FE7501;

	text-decoration: none;

	}

	

#ignite-wrapper {

	width:960px;

	margin: 0px auto;

	}

	

/* GLOBALS ------------------------------------------------------------------------------------------- */

#jive-wrapper {

	min-width:960px;

}

#jive-body #jive-breadcrumb {

	font-size: 9pt;

	color: #999;

	padding-bottom: 0px;

	}

#jive-breadcrumb span {

	padding-top: 15px;

}

#jive-breadcrumb a {

	color:#999;

	padding:0pt 1px;

	text-decoration: none;

}

#jive-breadcrumb a.jive-breadcrumb-last {

	color:#FE7501;

	text-decoration: none;

}

#jive-breadcrumb a:hover {

	color: #FE7501;

	text-decoration: underline;

}



.jive-table-rss-link {

	padding-top:20px;

	}	

	

.jive-content-block2-container {

	-moz-border-radius: 8px;

}



.jive-link-privatemsg {

        background-image: none;

}



/* MAIN CONTAINER ------------------------------------------------------------------------------------ */	

#jive-body-wrapper {

	background: #fff;

	background-image: none;

}

#jive-body, #jive-body-full {

	background: #fff;

	background-image: none;

}



/* set current page nav, define first id in body tag of each section */

#home #nav01 a, 

#projects #nav02 a, 

#downloads #nav03 a, 

#community #nav04 a, 

#fans #nav05 a, 

#store #nav06 a, 

#support #nav07 a, 

#about #nav08 a {

	background: transparent url(/images/ignite_nav-on.gif) no-repeat center;

	font-weight: bold;

	text-decoration: none;

	}





/* MASTHEAD  ---------------------------------------------------------------------------------------- */

#jive-global-header {

	background: #000000 url(/community/themes/ignite/images/ignite_hdr-bg.gif) no-repeat scroll 0%;

	border-bottom:medium none;

	padding:0px;

	width: 100%;

}

a#jive-global-header-logo {

	background:transparent url(/community/themes/ignite/images/ignite_logo.gif) no-repeat scroll 0%;

	scroll 0%;

	float:left;

	height:85px;

	margin-bottom:1px;

	width:225px;

}

#ignite_jive {

	width: 184px;

	height: 11px;

	float: right;

	margin: 0px;

	padding: 13px 20px 0px 0px;

	background: #000 url(/community/themes/ignite/images/ignite_hdr-jivecommunity.gif) no-repeat bottom left;

	}



/* IGNITE NAVIGATION BAR ------------------------------------------------------------------------ */	

#ignite_nav {

	height: 48px;

	clear: both;

	margin: 0px 0px 0px 0px;

	padding: 0px;

	background: #fff url(/community/themes/ignite/images/ignite_nav-bg.gif) repeat-x;

	}

#ignite_nav ol {

	list-style-type:none;

	margin:0px 0px 0px 5px;

	padding:0px;

}

#ignite_nav ol li {

	background:transparent url(/community/themes/ignite/images/ignite_nav-div.gif) no-repeat scroll right center;

	color:#FFFFFF;

	float:left;

	font-size:11pt;

	margin:4px 0px 0px;

	padding:10px 2px 10px 0px;

}

#ignite_nav ol li a, #ignite_nav ol li a:visited {

	color:#FFFFFF;

	font-size:11pt;

	margin:0px;

	padding:10px 11px;

	text-decoration:none;

}

#ignite_nav ol li a:hover, #ignite_nav ol li a:active {

	text-decoration:underline;

}

#ignite_nav ol li a.ignite_nav_current, #ignite_nav ol li a.ignite_nav_current:hover {

	background:transparent url(/community/themes/ignite/images/ignite_nav-on.gif) no-repeat scroll center;

	font-weight:bold;

}

#home #nav01 a, #projects #nav02 a, #downloads #nav03 a, #forum #nav04 a, #fans #nav05 a, #store #nav06 a, #support #nav07 a, #about #nav08 a {

	background:transparent url(/community/themes/ignite/images/ignite_nav-on.gif) no-repeat scroll center;

	font-weight:bold;

	text-decoration:none;

}



/* USER BAR ------------------------------------------------------------------------ */

#jive-userbar {

	background-image: url(/community/themes/ignite/images/jive-x-userbar-bg.gif);

	}



#jive-userbar-right {

	background-image: url(/community/themes/ignite/images/jive-x-userbar-search-bg.gif);

	position: relative;

}



#jive-userbar-search {

	padding-top:16px;

}

#jive-userbar-search .jive-userbar-search-field {

    margin-left: 3pt;

}

#jive-userbar-print {

	margin:21px 10px 0px 0px;

}

#jive-userbar-print a {

    padding:0px 0pt 3px 20px;

}

/* COLUMNS ----------------------------------------------------------------------- */

#jive-body-maincol {

	margin-right:230px;

	padding:0px 22px;

}



/* FOOTER ------------------------------------------------------------------------ */

#jive-footer {

	color: #666;

	text-align: left;

	}

#jive-footer a {

	color: #FE7501;

	text-decoration: underline;

	}

#jive-footer a:hover {

	text-decoration: none;

	}

.jive-footer-nav {

	text-align: left;

}

.jive-footer-nav a {

	padding: 0px;

}

.jive-footer-copyright {

	text-align: right;

}



/* CONTAINERS -------------------------------------------------------------------- */

.jive-content-block-container {

	border: none;

	background-color: #FFF;

	background-image: none;

	}

.jive-content-block {

	border: none;

	border-top:1px solid #DADADA;

	-moz-border-radius: 0px;

	padding: 10px 0px 0px;

	margin: 0px;

}	

.jive-content-block-container h3 {

	color:#555555;

	display:block;

	float:left;

	font-size:14pt;

	font-weight:normal;

	margin:0pt;

	padding:5px 0px 5px 0px;

}

.jive-content-block-container span.jive-content-hdr-link {

	padding: 10px 0px 0px 0px;

	}

.jive-content-block-container span.jive-content-hdr-link-home {

	border: none 0px;

	background: none;

	padding:12px 4px 8px 10px;

	}



html > body #jive-bodyhome-maincol {

	margin-left:350px;

}





/* COMMUNITY SIDE CONTAINER  */

#jive-community-list-padding {

	padding: 0px;

	}

.jive-community-item-content {

	padding:0px;

}

.jive-communitylist-big {

	background: none;

}

.jive-communitylist-big, .jive-communitylist-big-new {

	height:20px;

	padding:4px 0pt 2px 0px;

}





.jive-communitylist-sm {

	background:transparent url(/community/themes/ignite/images/ignite_blog_icon_tags.gif) no-repeat scroll left center;

}

#jive-community-list-short ul {

	margin:5px 0pt 5px 0px;

	padding:0pt;

}

#jive-community-list-short ul li p {

	margin: 3px 0px 10px 19px;

}



/* INTROS ---------------------------------------------------------- */

#jive-body-intro {

	padding:10px 0px 25px;

}

#jive-body-home #jive-body-intro-content {

	display: none;

}

#jive-body-intro-main-hdr {

	padding:0px 0px 0px 20px;

}



#jive-body-intro h1 {

	color: #555;

	}

#jive-body-intro-content p {

	padding:0px 0px 10px 10px !important;

}

.jive-body-intro-home #jive-body-intro-content p {

	padding: 0px !important;

} 

#jive-body-intro-content a.jive-link-more {

	margin-left: 20px;

	padding: 1px 0px 1px 12px;

}





.jive-body-intro-community {

	background: none;

}

.jive-body-intro-community, .jive-body-intro-profile {

	border: none 0px;

	border-bottom:1px solid #D0D0D0;

	-moz-border-radius: 0px;

	padding:12px 12px 0pt;

}	

.jive-icon-community-big {

	background: none;

	padding:4px 0pt 4px 10px;

	}

.jive-body-intro-community .jive-body-tabbar {

	margin-left:0px;

}



/* SIDEBARS ------------------------------------------------------- */

.jive-sidebar .jive-sidebar-body {

	background:#FFFFFF none repeat scroll 0%;

	border:none;

	font-size:8pt;

	overflow:hidden;

	width:202px;

}

.jive-sidebar .jive-sidebar-body-actions, .jive-sidebar .jive-sidebar-body-filter {

	background: #FDFDFD url(/community/themes/ignite/images/jive-action-bar-bg.gif) repeat-x left top;

	border:1px solid #DCDCDC;

	-moz-border-radius: 10px;

	width: 202px;

}





.jive-sidebar .jive-sidebar-body h4 {

	border-bottom:1px solid #EDEDED;

	font-size:11pt;

	margin:0px 0px 10px;

	padding:8px 10px 6px 0px;

}

.jive-sidebar .jive-sidebar-body ul {

	padding:0px 6px 3px 0px;

}

.jive-sidebar .jive-sidebar-body p {

	padding:2px 0px 8px;

}



.jive-sidebar .jive-sidebar-body-actions h4 {

	border-bottom: none;

	margin: 0px;

	padding: 8px 10px 15px;

	}

.jive-sidebar .jive-sidebar-body-actions ul {

	padding:0px 6px 12px 10px;

}

.jive-sidebar .jive-sidebar-body-actions p {

	padding:2px 10px 8px;

}





/* DISCUSSIONS --------------------------------------------------- */

.jive-thread-post {

	border: none;

	-moz-border-radius: 10px;

	}

.jive-thread-info {

	background: none;

	border-bottom:1px solid #D5E3EE;

	margin:2px 0px 0px;

	padding:10px 5px 10px 10px;

}

.jive-thread-post-subject-content {

	background: #D5E3EE url(/community/themes/ignite/images/jive-thread-hdr-bg.png) repeat-x left top;

	clear:both;

	margin:0px 0px 15px;

	padding:15px;

}





/* BLOGS ----------------------------------------------------------- */

.jive-blog-info, .jive-groupblog-info {

	-moz-border-radius-topleft: 10px;

	-moz-border-radius-topright: 10px;

	border-top: 1px #e7f7d6 solid;

	border-left: 1px #e7f7d6 solid;

	border-right: 1px #e7f7d6 solid;

	margin: 10px 0px 0px;

}

.jive-blog-post, .jive-groupblog-post {

	-moz-border-radius-bottomleft: 10px;

	-moz-border-radius-bottomright: 10px;

	border-bottom: 1px #e7f7d6 solid;

	border-left: 1px #e7f7d6 solid;

	border-right: 1px #e7f7d6 solid;

	width: 100%;

}



/* Blog Posts Page */

#jive-blogpost-content .jive-blog-post, .jive-groupblog-post {

	border-left: none;

	border-right: none;

	-moz-border-radius: 0px;

	}





/* DOCUMENTS -------------------------------------------------------- */

.jive-wiki {

	-moz-border-radius: 10px;

	background: #f8f4e6 url(/community/themes/ignite/images/jive-wiki-hdr-bg.png) repeat-x left top;

	}







/* Group Chat Timer */

#ignite_nav_groupchat {

	float: left;

	font-size: 8pt;

	color: #333;

	}

#ignite_nav_groupchat strong {

	color: #fff;

	font-size: 9pt;

	}

#ignite_nav_groupchat a {

	color: #fff;

	}

#ignite_nav_groupchat span {

	display: block;

	float: left;

	padding: 4px 6px 3px 6px;

	margin-right: 8px;

	margin-top: 11px;

    }

#ignite_nav_groupchat span.ignite_nav_groupchat-block {

	background-color: #fcb106;

	border: 1px solid #ffe793;

	-moz-border-radius: 3px;

	margin-left: 10px;

	padding-top: 3px;

    }

    

#jive-people-search {

    padding:10px !important;

}