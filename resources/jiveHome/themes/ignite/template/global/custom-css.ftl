	margin-top: 13px;

	}

	

/* GLOBALS ------------------------------------------------------------------------------------------- */

#jive-wrapper {

	min-width:960px;

}

#user-bar-wrapper {
	padding-top: 0;
}

#jive-body #jive-breadcrumb {

	font-size: 9pt;

	color: #999;

	padding-bottom: 5px;

	}

#jive-breadcrumb span {

	padding-top: 15px;

}

#jive-breadcrumb a {

	color:#999;

	padding:0pt 1px;

	text-decoration: none;

}

#jive-breadcrumb a.jive-breadcrumb-last {

	color:#FE7501;

	text-decoration: none;

}

#jive-breadcrumb a:hover {

	color: #FE7501;

	text-decoration: underline;

}

.jive-customize-info, .jive-customize-details {

	margin-top: -24px;

}

#jive-overview-customize-info, #jive-overview-customize-details {

	margin-top: -24px;

}

.jive-table-rss-link {

	padding-top:20px;

	}	

	

/* MAIN CONTAINER ------------------------------------------------------------------------------------ */	

#jive-body-wrapper {

	background: #fff;

	background-image: none;

}

#jive-body, #jive-body-full {

	background: #fff;

	background-image: none;

}



/* set current page nav, define first id in body tag of each section */

#home #nav01 a, 

#projects #nav02 a, 

#downloads #nav03 a, 

#community #nav04 a, 

#fans #nav05 a, 

#store #nav06 a, 

#support #nav07 a, 

#about #nav08 a {

	background: transparent url(/images/ignite_nav-on.gif) no-repeat center;

	font-weight: bold;

	text-decoration: none;

	}





/* MASTHEAD  ---------------------------------------------------------------------------------------- */

#jive-global-header {

	background: #000000 url(/community/themes/ignite/images/ignite_hdr-bg.gif) no-repeat scroll 0%;

	border-bottom:medium none;

	padding:0px;

	width: 100%;

}

#jive-global-header-texture {

	padding:0px;

}

a#jive-global-header-logo {

	background:transparent url(/community/themes/ignite/images/ignite_logo.gif) no-repeat scroll 0%;

	scroll 0%;

	float:left;

	height:85px;

	margin-bottom:1px;

	width:225px;

}

#ignite_jive {

	width: 184px;

	height: 48px;

	float: right;

	margin: 0px;

	padding: 13px 20px 0px 0px;

	background: #000 url(/community/themes/ignite/images/ignite_hdr-jivecommunity.gif) no-repeat bottom left;

	}



/* IGNITE NAVIGATION BAR ------------------------------------------------------------------------ */	

#ignite_nav {

	height: 48px;

	clear: both;

	margin: 0px 0px 0px 0px;

	padding: 0px;

	background: #fff url(/community/themes/ignite/images/ignite_nav-bg.gif) repeat-x;
	font-family: Arial,Helvetica,sans-serif;

	}

#ignite_nav ol {

	list-style-type:none;

	margin:0px 0px 0px 5px;

	padding:0px;

}

#ignite_nav ol li {

	background:transparent url(/community/themes/ignite/images/ignite_nav-div.gif) no-repeat scroll right center;

	color:#FFFFFF;

	float:left;

	font-size:11pt;

	margin:4px 0px 0px;

	padding:10px 2px 10px 0px;

}

#ignite_nav ol li a, #ignite_nav ol li a:visited {

	color:#FFFFFF;

	font-size:11pt;

	margin:0px;

	padding:10px 11px;

	text-decoration:none;

}

#ignite_nav ol li a:hover, #ignite_nav ol li a:active {

	text-decoration:underline;

}

#ignite_nav ol li a.ignite_nav_current, #ignite_nav ol li a.ignite_nav_current:hover {

	background:transparent url(/community/themes/ignite/images/ignite_nav-on.gif) no-repeat scroll center;

	font-weight:bold;

}

#home #nav01 a, #projects #nav02 a, #downloads #nav03 a, #forum #nav04 a, #fans #nav05 a, #store #nav06 a, #support #nav07 a, #about #nav08 a {

	background:transparent url(/community/themes/ignite/images/ignite_nav-on.gif) no-repeat scroll center;

	font-weight:bold;

	text-decoration:none;

}





/* COLUMNS ----------------------------------------------------------------------- */

#jive-body-maincol {

	margin-right:230px;

	padding:0px 22px;

}



/* FOOTER ------------------------------------------------------------------------ */

#jive-footer {

	color: #666;

	padding-left: 10px;

	padding-right: 10px;

	text-align: left;

	}

#jive-footer a {

	color: #FE7501;

	text-decoration: underline;

	}

#jive-footer a:hover {

	text-decoration: none;

	}

.jive-footer-nav {

	text-align: left;

}

.jive-footer-nav a {

	padding: 0px;

}

.jive-footer-copyright {

	text-align: right;

}



/* CONTAINERS -------------------------------------------------------------------- */

.jive-content-block-container {

	border: none;

	background-color: #FFF;

	background-image: none;

	}

.jive-content-block {

	border: none;

	border-top:1px solid #DADADA;

	-moz-border-radius: 0px;

	padding: 10px 0px 0px;

	margin: 0px;

}	

